policy_module(ulogd, 1.0)
gen_require(`
  type ulogd_t;
  type passwd_file_t;
  type sssd_var_lib_t;
  type systemd_userdbd_runtime_t;
')

#============= ulogd_t ==============
allow ulogd_t passwd_file_t:file { read };
allow ulogd_t sssd_var_lib_t:dir { search };
allow ulogd_t systemd_userdbd_runtime_t:dir { read };
